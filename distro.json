{
    "name": "MovieSpace"
,   "author": "Sohankd"
,   "modulePath":{
        "Modules": "src/Modules"
    ,   "Vendor": "src/Vendor" 
    ,   "DistributionFolder": "Dist"
    }
,   "modules":[
        "Modules/Application"
    ,   "Modules/Layout"
    ,   "Modules/Home"
    ,   "Modules/Error.Management"
    ,   "Modules/Header"
    ,   "Modules/Footer"
    ,   "Modules/Categories"
    ,   "Modules/ProductDetails"
    ,   "Modules/Vendors.Extras"
    ,   "Modules/Models"
    ,   "Modules/GlobalViews"
    ,   "Modules/MediaCollections"
    ,   "Vendor/backbone"
    ,   "Vendor/backbone.radio"
    ,   "Vendor/bootstrap"
    ,   "Vendor/Marionette"
    ,   "Vendor/jquery"
    ,   "Vendor/text"
    ,   "Vendor/underscore"
    ,   "Vendor/handlebars"
    ]
,   "javascript":{
        "folder": "JavaScript"
    ,   "entrypoints":[
            "ajaxSetup"
        ,   "Backbone.Model"
        ,   "Backbone.Collection"
        ,   "bootstrap.custom"
        ,   "Error.Management"
        ,   "Handlebars.Extra"
        ,   "Home"
        ,   "Marionette.View"
        ,   "MediaCollections"
        ,   "ProductDetails"
        ]
    }
,   "amdConfig":{
        "shim":{
            "jquery": {
                "exports": "jquery"
            }
        ,   "underscore": {
                "exports": "_"
            }
        ,   "Handlbars": {
                "exports": "Handlebars"
            }
        ,   "Bootstrap":{
                "deps": ["jquery","popper"]
            }
        ,   "backbone": {
                "deps": ["jquery","underscore"]
            ,   "exports": "backbone"
            }
        ,   "backbone.radio": {
                "deps": ["backbone"]
            ,   "exports": "backbone.radio"
            }
        ,   "Marionette": {
                "deps": ["backbone","backbone.radio"]
            ,   "exports": "Marionette"
            }
        }
    }
,   "amdLoader": {
        "almond": "src/Vendor/almond/almond"
    }
,   "template":{
        "folder": "Template"
    ,   "extname": "hbs"
    }
,   "scss":{
        "folder": "Sass"
    ,   "exportFile": "style.css"
    ,   "mainFile": "Application.scss"
    }
,   "compiledFilesFolder": "precompiled"
,   "buildFile": "bundle.js"
}