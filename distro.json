{
    "name": "MovieSpace"
,   "author": "Sohankd"
,   "modulePath":{
        "Modules": "src/Modules"
    ,   "vendor": "src/vendor" 
    ,   "DistributionFolder": "Dist"
    }
,   "modules":[
        "Modules/Application"
    ,   "Modules/Layout"
    ,   "Modules/Home"
    ,   "Modules/Error.Management"
    ,   "Modules/Header"
    ,   "Modules/Footer"
    ,   "Modules/Categories"
    ,   "Modules/ProductDetails"
    ,   "Modules/Vendors.Extras"
    ,   "Modules/Models"
    ,   "Modules/GlobalViews"
    ,   "Modules/MediaCollections"
    ,   "vendor/backbone"
    ,   "vendor/backbone.radio"
    ,   "vendor/bootstrap"
    ,   "vendor/Marionette"
    ,   "vendor/jquery"
    ,   "vendor/text"
    ,   "vendor/underscore"
    ,   "vendor/handlebars"
    ]
,   "javascript":{
        "folder": "JavaScript"
    ,   "exportFile": "javascript.js"
    ,   "entrypoints":[
            "ajaxSetup"
        ,   "Backbone.Model"
        ,   "Backbone.Collection"
        ,   "bootstrap.custom"
        ,   "Error.Management"
        ,   "Handlebars.Extra"
        ,   "Home"
        ,   "Marionette.View"
        ,   "MediaCollections"
        ,   "ProductDetails"
        ]
    ,   "amdConfig":{
            "paths":{
                "jquery": "jquery.min",
                "underscore": "underscore.min",
                "Handlebars": "handlebars.min",
                "backbone": "backbone.min",
                "backbone.radio": "backbone.radio.min",
                "Marionette": "backbone.marionette.min"
            }
        ,   "shim":{
                "jquery": {
                    "exports": "jquery"
                }
            ,   "underscore": {
                    "exports": "_"
                }
            ,   "Handlbars": {
                    "exports": "Handlebars"
                }
            ,   "Bootstrap":{
                    "deps": ["jquery","popper"]
                }
            ,   "backbone": {
                    "deps": ["jquery","underscore"]
                ,   "exports": "backbone"
                }
            ,   "backbone.radio": {
                    "deps": ["backbone"]
                ,   "exports": "backbone.radio"
                }
            ,   "Marionette": {
                    "deps": ["backbone","backbone.radio"]
                ,   "exports": "Marionette"
                }
            }
        }
    }
,   "amdLoader": {
        "almond": "src/vendor/almond/almond"
    ,   "require": "src/vendor/require/require"
    }
,   "template":{
        "folder": "Template"
    ,   "exportFile": "template.js"
    }
,   "scss":{
        "folder": "Sass"
    ,   "exportFile": "style.css"
    ,   "mainFile": "Application.scss"
    }
}